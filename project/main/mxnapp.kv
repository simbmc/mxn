<AppActionBar>:
    pos_hint: {'top':1}
    size_hint: (1., 0.1)
    ActionView:
        use_separator: True
        ActionMenu:
            title: 'mxnix'
            with_previous: False
            app_icon: './rwth.png'
            app_icon_height: dp(1000)
        ActionOverflow:
        ActionButton:
            text: 'cross section'
            on_press:app.show_cross_section_editor()
        ActionButton:
            text: 'reinforcement'
            on_press:app.show_reinforcement_editor()
        ActionButton:
        	text: 'explorer'
        	on_press:app.show_explorer()
        ActionButton:
        	text: 'mxn envelope'
        	on_press:app.show_mxnEmelope()
        ActionButton:
            text: 'material'
            on_press:app.show_material_editor()